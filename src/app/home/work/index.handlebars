<div class="my-work my-experience">
	<div class="e-title nav-name nav-work" data-id="nav-work">
		<label class="middle">
		<img src="data:image/png;base64," alt="">	
		</label>
		<span>工作经历</span>
	</div>
	<div class="work-list e-list">
		<ul class="n">
		{{#each list}}
			<li>
				<div class="l-sign"></div>
				<div class="l-info">
					<div class="l-time">{{datetime}}</div>
					<div class="l-name">{{name}}</div>
					<div class="l-post">{{post}}</div>
					<div class="l-summary">{{{summary}}}</div>
				</div>
			</li>
		{{/each}}
		</ul>
	</div>
</div>