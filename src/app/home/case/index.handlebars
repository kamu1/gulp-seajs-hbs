<div class="my-case my-experience">
	<div class="e-title nav-name nav-case" data-id="nav-case">
		<label class="middle">
		<img src="data:image/png;base64," alt="">	
		</label>
		<span>项目案例</span>
	</div>
	<div class="e-list c-list">
		<ul class="n">
			{{#each list}}
			{{#if title}}
			<li>
				<div class="l-info l-project">
					<div class="l-name">
						{{#if links}}
						<div class="l-menu" tabindex="-1">
							<span tabindex="-1"></span>
							<div class="J-link-btn">
							{{#each links}}
								<span class="{{{class}}}" data-link="{{url}}">{{#if title}}{{title}}{{else}}访问·PC版{{/if}}</span>
							{{/each}}
							</div>
						</div>
						{{/if}}
						{{title}}
					</div>
					{{#if images}}
					<div class="l-imgs swiper-container swiper-V">
						<div class="swiper-wrapper">
							<div class="l-img swiper-slide">
							{{#each images}}
							<span><img data-src="{{url}}" class="lazy-load" alt=""></span>
							{{/each}}
							</div>
						</div>
					</div>
					<div class="img-tri"></div>
					{{/if}}
					<div class="l-post">{{label}}</div>
					<div class="l-summary">{{{summary}}}</div>
				</div>
			</li>
			{{/if}}
			{{/each}}
		</ul>
	</div>
</div>