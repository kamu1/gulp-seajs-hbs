<div class="my-experience">
	<div class="e-title nav-name nav-project" data-id="nav-project">
		<label class="middle">
		<img src="data:image/png;base64," alt="">	
		</label>
		<span>项目经验</span>
	</div>
	<div class="e-list">
		<ul class="n">
		{{#each list}}
			<li>
				<div class="l-sign"></div>
				<div class="l-info l-project">
					{{#if datetime}}<div class="l-time">{{datetime}}</div>{{/if}}
					{{#if name}}<div class="l-name">{{name}}</div>{{/if}}
					{{#if environment}}<div class="l-post">{{environment}}</div>{{/if}}
					{{#if pDescription}}<div class="l-summary" data-label="项目描述：">{{{pDescription}}}</div>{{/if}}
					{{#if jDescription}}<div class="l-content" data-label="职责描述：">{{{jDescription}}}</div>{{/if}}
				</div>
			</li>
		{{/each}}
		</ul>
	</div>
</div>