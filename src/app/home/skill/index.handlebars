<div class="my-skill my-experience">
	<div class="e-title nav-name nav-skill" data-id="nav-skill">
		<label class="middle">
		<img src="data:image/png;base64," alt="">	
		</label>
		<span>IT技能</span>
	</div>
	<div class="s-list">
		<ul class="n">
			{{#each list}}
			<li>
				<label>
					<input type="radio" name="skill-radio" id="">
					<div>
					{{#if imgUrl}}
						<div class="s-img middle">
							<img src="{{imgUrl}}" alt="">
						</div>
					{{/if}}
						<div class="s-text">
							<div class="s-title">{{title}}</div>
							<div class="s-summary">{{{summary}}}</div>
						</div>
					</div>
				</label>
			</li>
			{{/each}}
		</ul>
	</div>
</div>